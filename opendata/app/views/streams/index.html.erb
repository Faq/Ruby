<p id="notice"><%= notice %></p>

<h1>Streams</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Country</th>
      <th>Twoweek cumulative</th>
      <th>Departure</th>
      <th>Self isolation</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @streams.each do |stream| %>
      <tr>
        <td><%= stream.date %></td>
        <td><%= stream.country %></td>
        <td><%= stream.twoweek_cumulative %></td>
        <td><%= stream.departure %></td>
        <td><%= stream.self_isolation %></td>
        <td><%= link_to 'Show', stream %></td>
        <td><%= link_to 'Edit', edit_stream_path(stream) %></td>
        <td><%= link_to 'Destroy', stream, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Stream', new_stream_path %>
